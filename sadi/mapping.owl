<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#">
    
  <owl:Ontology rdf:about="">
  </owl:Ontology>

  <!-- <owl:DatatypeProperty rdf:about="#file1"/> -->
  <!-- <owl:DatatypeProperty rdf:about="#file2"/> -->
  <!-- <owl:DatatypeProperty rdf:about="#mapping"/> -->
  
  <owl:Class rdf:ID="Fastq_files">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#fastq_1"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#fastq_2"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#adapter"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#genome"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#offset"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>

  <owl:Class rdf:ID="Bam_file">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#mapping"/>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  
 
</rdf:RDF>

