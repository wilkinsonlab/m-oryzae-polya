mendeley-tags = {Class\_GI,Data base,Function,RNA,database,lincrna,lncRNAdb database,ncRNA},
pages = {D146--D151},
pmid = {21112873},
title = {{lncRNAdb: a reference database for long noncoding RNAs.}},
volume = {39},
