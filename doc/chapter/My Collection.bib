mendeley-tags = {Folder - Sequencing,Genome- Fungal,Nucleic Acid Conformation,RNA- Fungal,RNA- Untranslated,Saccharomyces cerevisiae,Sequence Analysis- RNA,Sequence Homology- Nucleic Acid,bioinformatics,computational\_biology,fungi,ncrna,noncoding\_rna,prediction,saccharomyces,thermodynamic,yeast},
