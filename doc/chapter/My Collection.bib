@article{Schmidt2010,
abstract = {The addition of nontemplated nucleotides, particularly adenylyl and uridylyl residues, to the 3' ends of RNA substrates has been the focus of much attention in recent years, and these studies have generated some intriguing surprises. In addition to the well-known canonical poly(A) polymerase (PAP) that polyadenylates mRNAs prior to export from the nucleus to the cytoplasm, a separate class of noncanonical poly(A) polymerases has emerged over the past decade. Studies on various organisms have led to the realization that these noncanonical PAPs, which are conserved from yeast to mammals, play crucial and diverse roles in the regulation of gene expression. Here we review the current knowledge of these enzymes, with an emphasis on the human proteins, and highlight recent discoveries that have implications far beyond the understanding of RNA metabolism itself.},
author = {Schmidt, Marie-Jo\"{e}lle and Norbury, Chris J},
doi = {10.1002/wrna.16},
